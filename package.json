{
  "name": "tree-sitter-vsif",
  "version": "0.0.1",
  "description": "vsif grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "repository": "https://github.com/vmorillon/tree-sitter-vsif",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "files": [
    "/src/",
    "/queries/",
    "/bindings/node/",
    "/binding.gyp",
    "/grammar.js"
  ],
  "tree-sitter": [
    {
      "scope": "source.vsif",
      "file-types": [
        "vsif"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
