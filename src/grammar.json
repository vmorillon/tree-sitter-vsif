{
  "name": "vsif",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "named_scope"
          },
          {
            "type": "SYMBOL",
            "name": "pair"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "STRING",
                "value": "--"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "named_scope": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "_bare_string"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "_bare_string"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "named_scope"
              },
              {
                "type": "SYMBOL",
                "name": "pair"
              },
              {
                "type": "PATTERN",
                "value": "\\r?\\n"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "key": {
      "type": "SYMBOL",
      "name": "_bare_string"
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "integer": {
      "type": "PATTERN",
      "value": "[+-]?(0|[1-9](_?[0-9])*)"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_bare_string"
        },
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_tagged_string"
        }
      ]
    },
    "_bare_string": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"\\n]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_tagged_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<text>"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^<]"
          }
        },
        {
          "type": "STRING",
          "value": "</text>"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

